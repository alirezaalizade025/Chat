{
  "name": "my-project",
  "version": 2,
  "builds": [
    {
      "src": "main.go",
      "use": "@vercel/go"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "api/(.*)",
      "dest": "api/index.go"
    },
    {
      "src": "/fr",
      "dest": "/frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_BASE_URL": "https://jora-smoky.vercel.app/api"
  }
}
